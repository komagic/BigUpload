{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "declaration": false,
    "outDir": null,
    "types": ["jest", "node"],
    "esModuleInterop": true,
    "allowJs": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "src/**/__tests__/**/*",
    "src/setupTests.ts",
    "src/types/jest.d.ts"
  ],
  "exclude": []
} 